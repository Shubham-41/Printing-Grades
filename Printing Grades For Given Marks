#include<stdio.h>
int main(int argc, char const *argv[])
{
     int marks;
     printf("Enter Your Marks :");
     scanf("%d", &marks);

    if ( 85 <= marks && marks <= 100  )
    {
        printf("Grade A");
    }
    else if ( 70 <= marks && marks <= 84)
    {
        printf("Grade B");
    }
    else if (55 <= marks && marks <= 69)
    {
        printf("Grade C");
    }
    else if (40 <= marks && marks <= 54)
    {
        printf("Grade D");
    }
    else
   {
        printf("Grade F");
   }
    


    return 0;
}
